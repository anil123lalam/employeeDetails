CREATE TABLE `employee_details` (
  `pklId` int NOT NULL AUTO_INCREMENT,
  `employee_code` varchar(45) DEFAULT NULL,
  `emp_firstName` varchar(45) DEFAULT NULL,
  `emp_lastName` varchar(45) DEFAULT NULL,
  `emp_email` varchar(45) DEFAULT NULL,
  `emp_doj` varchar(45) DEFAULT NULL,
  `emp_salary` double DEFAULT NULL,
  PRIMARY KEY (`pklId`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci;

CREATE TABLE `phone_numbers` (
  `pklId` int NOT NULL AUTO_INCREMENT,
  `phone_number` int DEFAULT NULL,
  `employee_id` int DEFAULT NULL,
  PRIMARY KEY (`pklId`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci;
